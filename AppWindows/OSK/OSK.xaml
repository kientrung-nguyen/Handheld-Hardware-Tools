<Window x:Class="Everything_Handhelds_Tool.AppWindows.OSK.OSK"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
      
        xmlns:local="clr-namespace:Everything_Handhelds_Tool.AppWindows.OSK"
              mc:Ignorable="d"
        WindowStyle="None"
        AllowsTransparency="True"
        Left="0"
        Top="2000"
        Topmost="True"
        Focusable="False"
        ShowInTaskbar="True"
                ShowActivated="False"
        ResizeMode="NoResize"
  xmlns:outlineTB="clr-namespace:Everything_Handhelds_Tool.Classes"
        Title="OSK" Height="0" Width="0"
      
        Opacity=".95"  Background="{DynamicResource {x:Static SystemColors.ControlDarkDarkBrushKey}}"
        >
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="30"/>r
            <RowDefinition Height="*"/>
        </Grid.RowDefinitions>
        <outlineTB:OutlinedTextBlock x:Name="outlineTextblock" HorizontalAlignment="Center" FontSize="20" Fill="Blue" />

        <DockPanel Grid.Row="1">
            <Frame Name="frame" NavigationUIVisibility="Hidden"/>
        </DockPanel>
    </Grid>
    

</Window>
