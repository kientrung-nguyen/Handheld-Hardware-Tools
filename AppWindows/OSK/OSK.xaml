<Window x:Class="Everything_Handhelds_Tool.AppWindows.OSK.OSK"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Everything_Handhelds_Tool.AppWindows.OSK"
              mc:Ignorable="d"
        WindowStyle="None"
        AllowsTransparency="True"
        Left="0"
        Topmost="True"
        Focusable="False"
        ShowInTaskbar="True"
                ShowActivated="False"
        ResizeMode="NoResize"
        
        Title="OSK" Height="240" Width="1000"
        Opacity=".95"  Background="{DynamicResource {x:Static SystemColors.ControlDarkDarkBrushKey}}"
        >
    <DockPanel>
        <Frame Name="frame" NavigationUIVisibility="Hidden"/>
    </DockPanel>
</Window>
